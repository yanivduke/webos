{
  "id": "dark-mode",
  "name": "Dark Mode",
  "description": "Modern dark theme with blue accents and reduced eye strain",
  "version": "1.0.0",
  "colors": {
    "primary": "#1a73e8",
    "background": "#2d2d2d",
    "text": "#ffffff",
    "textInverse": "#000000",
    "border": "#555555",
    "borderLight": "#777777",
    "borderDark": "#000000",
    "windowChrome": "#3d3d3d",
    "windowTitle": "#1a73e8",
    "windowTitleText": "#ffffff",
    "highlight": "#1a73e8",
    "highlightText": "#ffffff",
    "shadow": "#1a1a1a",
    "accent": "#ff9800",
    "success": "#4caf50",
    "warning": "#ffc107",
    "error": "#f44336",
    "disabled": "#666666",
    "inputBg": "#1a1a1a",
    "inputText": "#ffffff",
    "inputBorder": "#555555",
    "scrollbarTrack": "#2d2d2d",
    "scrollbarThumb": "#555555",
    "menuBg": "#3d3d3d",
    "menuText": "#ffffff",
    "menuHighlight": "#1a73e8",
    "menuHighlightText": "#ffffff"
  },
  "fonts": {
    "primary": "'Press Start 2P', monospace",
    "secondary": "monospace",
    "size": {
      "small": "8px",
      "medium": "10px",
      "large": "12px"
    }
  },
  "windowChrome": {
    "borderWidth": "2px",
    "borderRadius": "0px",
    "shadow": "0 4px 8px rgba(0, 0, 0, 0.5)",
    "titleBarHeight": "20px"
  },
  "spacing": {
    "small": "4px",
    "medium": "8px",
    "large": "12px"
  },
  "animation": {
    "duration": "0.1s",
    "easing": "ease-out"
  }
}
